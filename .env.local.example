# 윈도우 로컬 테스트용 커스텀 API 설정
AI_PROVIDER=custom

# 커스텀 API 설정
CUSTOM_API_KEY=your_api_key_here
CUSTOM_API_URL=http://your-local-api-server:port/endpoint
CUSTOM_MODEL=your_model_name
CUSTOM_API_FORMAT=custom
CUSTOM_HEADERS={"Authorization": "Bearer your_token", "Content-Type": "application/json"}

# 서버 설정
PORT=5000
HOST=127.0.0.1
NODE_ENV=development

# 세션 보안
SESSION_SECRET=local_development_secret_key_change_in_production